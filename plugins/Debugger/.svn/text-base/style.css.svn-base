.DebugInfo {
   clear: both;
   background: #eeeeee;
   color: #000;
   font-weight: bold;
   border-top: 1px solid #bbbbbb;
   padding: 10px 20px;
   margin: 0;
   text-align: left;
}
.DebugInfo h2, .DebugInfo h3 {
   font-size: 140%;
   font-weight: bold;
   margin: 0;
   padding: 4px 0;
}
.DebugInfo pre {
   border-radius: 2px;
   -moz-border-radius: 2px;
   -webkit-border-radius: 2px;
   font-family: monospace;
   padding: 4px;
   margin: 4px 0px 8px;
   overflow: auto;
   font-size: 12px;
   font-weight: normal;
   background: #fafafa;
   color: #000;
}
.DebugInfo small {
   padding: 0 4px;
   margin: 0 10px;
   font-family: trebuchet ms;
   font-size: 12px;
   font-weight: normal;
   color: #ff0084;
}